<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- FAB with wooden/bell coin style -->
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/icecream_accent_dark"/>
            <corners android:radius="32dp"/>
        </shape>
    </item>
    <item
        android:top="3dp"
        android:bottom="0dp"
        android:left="0dp"
        android:right="0dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/icecream_accent"/>
            <corners android:radius="32dp"/>
        </shape>
    </item>
</layer-list>


